<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-4.0.xsd">

	<context:annotation-config />
	<bean class="com.springcore.lifecycle.Samosa" name="s1"
		init-method="hye" destroy-method="bye">
		<property name="price" value="10" />
	</bean>
	<bean class="com.springcore.lifecycle.Pepsi" name="p1">
		<property name="price" value="40" />
	</bean>
	<bean class="com.springcore.lifecycle.Napkin" name="n1">
		<property name="price" value="4" />
	</bean>
</beans>
	